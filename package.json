{
  "name": "expensify",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "heroku-postbuild": "yarn run build:prod",
    "serve": "webpack-dev-server",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "start-dev": "cross-env NODE_ENV=development nodemon server/server.js",
    "start-test": "cross-env NODE_ENV=test nodemon server/server.js",
    "playground": "concurrently \"live-server public --port=9000\" \"babel src/playground/playground.js --out-file=public/scripts/app.js --presets=env,react --watch\"",
    "build:dev": "webpack",
    "build:prod": "webpack -p --env production",
    "build-babel": "babel src/app.js --out-file=public/scripts/app.js --presets=env,react --watch",
    "dev-server": "concurrently --kill-others \"npm run serve\" \"npm run start-dev\"",
    "test-server": "concurrently --kill-others \"npm run serve-test\" \"npm run start-test\"",
    "test-watch": "cross-env NODE_ENV=test jest --config=jest.config.json --watchAll"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "css-loader": "^0.28.10",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "^4.12.1",
    "moment": "^2.21.0",
    "mongoose": "^5.0.13",
    "node-sass": "^4.7.2",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dates": "^16.3.6",
    "react-dom": "^16.2.0",
    "react-modal": "^3.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uuid": "^3.2.1",
    "validator": "^9.4.1",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.3",
    "jest": "^22.4.2",
    "nodemon": "^1.17.3",
    "react-test-renderer": "^16.2.0",
    "webpack-dev-server": "^2.11.1"
  }
}
